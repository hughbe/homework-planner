# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'Homework Planner' do
  use_frameworks!
  inhibit_all_warnings!

  project "src/Homework Planner.xcodeproj"

  pod 'Date+Previous'
  pod 'Date+WithoutTime'
  pod 'DayDatePicker'
  pod 'INSPhotoGallery'
  pod 'JTCalendar'
  pod 'SZTextView'
  
end

target 'today' do
  use_frameworks!
  inhibit_all_warnings!

  project "src/Homework Planner.xcodeproj"

  pod 'Date+Previous'
  pod 'Date+WithoutTime'

end
